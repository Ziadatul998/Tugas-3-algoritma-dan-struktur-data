for i in range(7, 0, -1):
    for j in range(1, i + 1):
        print(j, end=' ')
    print()
START
FOR i FROM 7 DOWNTO 1 DO
    FOR j FROM 1 TO i DO
        PRINT j WITH SPACE
    END FOR
    PRINT NEW LINE
END FOR
END https://drive.google.com/file/d/1EhlQcFhXMau2MGpitW9i9eolefT8bLPy/view?usp=drivesdk

